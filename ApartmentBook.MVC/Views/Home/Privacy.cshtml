@{
    ViewData["Title"] = "Privacy Policy";
}



<p>Use this page to detail your site's privacy policy.</p>
 <div>
    <canvas id="myChart"></canvas>
</div>


<script>

    const labels = ["1", "2", "3", "4", "5", "6", "7"];
    const data = {
      labels: labels,
      datasets: [
        {
          label: 'Dataset 1',
          data: [3,6,6,9,6,8,8],
          borderColor: "rgba(196,77,86,1)",
          backgroundColor: "rgba(196,77,86,1)",
        },
        {
          label: 'Dataset 2',
          data: [1,3,4,5,5,6,3],
          borderColor: "rgba(147,250,165,1)",
          backgroundColor: "rgba(46,204,113,1)",
        }
      ]
    };

    const config = {
      type: 'bar',
      data: data,
      options: {
        responsive: true,
        plugins: {
          legend: {
            position: 'top',
          },
          title: {
            display: true,
            text: 'Chart.js Bar Chart'
          }
        }
      },
    };

    const DATA_COUNT = 7;
    const NUMBER_CFG = [1,3,4,5,5,6,3];



    const myChart = new Chart(
        document.getElementById('myChart'),
        config
      );

</script>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
